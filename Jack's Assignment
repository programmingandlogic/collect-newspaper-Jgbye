import stanford.karel.*;

public class CollectNewspaperKarel extends SuperKarel {

	// You fill in this part
	public void run() 	{
		goToBeeper();
		pickBeeper();
		goBack();
	}
	
	private void goToBeeper()	{
		move();
		move();
		turnRight();
		move();
		turnLeft();
		move();
	}
	
	private void goBack()	{
		turnLeft();
		turnLeft();
		move();
		move();
		move();
		turnRight();
		move();
		turnRight();
	}
}
